<div class="col-xs-12 col-md-4 pull-right padding-top rtl">
  <%= render 'event' %>
</div>
<div class="col-xs-12 col-md-8 pull-left">
  <div class="calendar">

  </div>
</div>
<div class="col-xs-12" style="margin-bottom: 15px;">
  <%= form_tag(export_icalendar_path, method: 'post', class: 'rtl') do %>
        <input type="hidden" name="start_date" id="start_date_field"/>
        <input type="hidden" name="end_date" id="end_date_field"/>
        <input type="submit" value="خروجی iCalendar" class="btn btn-info btn-sm pull-left"/>
  <% end %>
</div>
